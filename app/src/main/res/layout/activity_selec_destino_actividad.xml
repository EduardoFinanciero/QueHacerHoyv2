<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#126C87"
    tools:context=".SelecDestinoActividad">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="140dp"
        android:layout_height="137dp"
        app:srcCompat="@drawable/logo"
        tools:layout_editor_absoluteX="-4dp"
        tools:layout_editor_absoluteY="1dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        style="@android:style/Widget.ImageButton"
        android:layout_width="374dp"
        android:layout_height="119dp"
        android:backgroundTint="#2A9FAA"
        android:scaleType="fitStart"
        app:srcCompat="@mipmap/ic_iconologin_foreground"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="240dp" />

    <ImageButton
        android:id="@+id/imageButton9"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="374dp"
        android:layout_height="119dp"
        android:backgroundTint="#2A9FAA"
        android:scaleType="fitStart"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        app:srcCompat="@mipmap/ic_aventura_foreground"
        tools:layout_editor_absoluteX="18dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="374dp"
        android:layout_height="119dp"
        android:backgroundTint="#2A9FAA"
        android:scaleType="fitStart"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9"
        app:srcCompat="@mipmap/ic_cine_foreground"
        tools:layout_editor_absoluteX="18dp" />

    <ImageButton
        android:id="@+id/imageButton8"
        style="@style/Widget.AppCompat.ImageButton"
        android:layout_width="374dp"
        android:layout_height="119dp"
        android:backgroundTint="#2A9FAA"
        android:scaleType="fitStart"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7"
        app:srcCompat="@mipmap/ic_museo_foreground"
        tools:layout_editor_absoluteX="16dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="91dp"
        android:layout_height="45dp"
        android:enabled="true"
        android:fontFamily="sans-serif-black"
        android:text="Restaurantes"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton5"
        app:layout_constraintTop_toTopOf="@+id/imageButton5"
        tools:layout_editor_absoluteX="160dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Lugares Turisticos"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton9"
        app:layout_constraintTop_toBottomOf="@+id/imageButton5"
        tools:layout_editor_absoluteX="160dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Cines"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton7"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9"
        tools:layout_editor_absoluteX="160dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Museos"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton8"
        app:layout_constraintTop_toBottomOf="@+id/imageButton7"
        tools:layout_editor_absoluteX="157dp" />

    <Spinner
        android:id="@+id/spinner2"
        android:layout_width="409dp"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="171dp" />

</androidx.constraintlayout.widget.ConstraintLayout>